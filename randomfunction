import java.util.Random;
public class randomfunction {
	public static void main(String[] args) { 
	      Random number = new Random();
	      /** use the Random() util to generate our identifier*/
	      int indentifier;
	      /**declare variable for our identifier */
	      indentifier = 1+number.nextInt(10);
	      /** Generates a number from 1 to 10*/
	      System.out.println(indentifier);
	      /** return our identifier */
	   }
}
/**
 * We need to change the parameters in line 9 for change in our bank of quotes.
 * We will be able to correlate the identifier with the quote in the database and then 
 * display it.
 */
